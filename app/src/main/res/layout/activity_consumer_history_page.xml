<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.myapp.stranderdlog.consumer_history_page"
    android:background="@drawable/back3">

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/global_button"
        android:src="@drawable/set_limit_icon"
        android:contentDescription="@string/abc_action_bar_home_description_format"
        android:layout_weight="1.0"
        android:onClick="Global_read"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/global_read_history"
        android:textSize="@dimen/abc_text_size_body_2_material"
        android:id="@+id/textView1"
        android:background="#ffffffff"
        android:gravity="center"
        android:textIsSelectable="false" />

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/local_btn"
        android:src="@drawable/set_limit_icon"
        android:contentDescription="@string/abc_action_bar_home_description_format"
        android:layout_weight="1.0"
        android:onClick="Local_read"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/local_reading_history"
        android:textSize="@dimen/abc_text_size_body_2_material"
        android:id="@+id/textView2"
        android:background="#ffffffff"
        android:gravity="center"
        android:textIsSelectable="false" />
</LinearLayout>
